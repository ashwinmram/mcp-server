{
    "name": "ashwinmram/mcp-pusher",
    "description": "Laravel package to push lessons learned to MCP server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mcp",
        "lessons-learned",
        "package"
    ],
    "authors": [
        {
            "name": "Ashwin M Ram",
            "email": "ashwinmram@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/console": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMcpPusher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMcpPusher\\MCPPusherServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
